<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="using:HlaeObsTools.ViewModels.Docks"
             xmlns:webview="clr-namespace:WebViewControl;assembly=WebViewControl.Avalonia"
             x:Class="HlaeObsTools.Views.Docks.BrowserSourceDockView"
             x:DataType="vm:BrowserSourceDockViewModel">
    <Border Background="#1E1E1E" Padding="0" Focusable="True">
        <webview:WebView Focusable="True" Address="{Binding Address}" />
    </Border>
</UserControl>
